#%RAML 1.0 DataType
type: object
properties:
  employeeId : integer
  employeeName : string
  employeeDept :
    type: string
    enum:
      - JAVA
      - MULE
  employeeSalary : integer
  employeeAddress : string
  employeePhoneno : 
    type: string
    pattern: ^\(?([0-9]{3})\)?[-. ]?([0-9]{3})[-. ]?([0-9]{4})$

    


  